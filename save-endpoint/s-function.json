{
  "name": "save-endpoint",
  "runtime": "nodejs4.3",
  "description": "Serverless Lambda function for project: offcourse",
  "customName": false,
  "customRole": false,
  "handler": "index.handler",
  "timeout": 6,
  "memorySize": 128,
  "authorizer": {},
  "custom": {
    "excludePatterns": []
  },
  "endpoints": [],
  "events": [
    {
      "name": "${newCourseStreamName}",
      "type": "kinesisstream",
      "config": {
        "streamArn": "${newCourseARNStream}",
        "batchSize": 1,
        "enabled": true
      }
    },
    {
      "name": "${courseStreamName}",
      "type": "kinesisstream",
      "config": {
        "streamArn": "${courseARNStream}",
        "batchSize": 1,
        "enabled": true
      }
    }
  ],
  "environment": {
    "SERVERLESS_PROJECT": "${project}",
    "SERVERLESS_STAGE": "${stage}",
    "SERVERLESS_REGION": "${region}"
  },
  "vpc": {
    "securityGroupIds": [],
    "subnetIds": []
  }
}